{
	"name": "CodeR",
	"type": "dark",
	"colors": {
		"editor.background": "#1E1E1E",
		"editor.foreground": "#D4D4D4",
		"editor.inactiveSelectionBackground": "#3a3d4165",
		"editorIndentGuide.background1": "#2E2E2E",
		"editorIndentGuide.activeBackground1": "#3E3E3E",
		"editor.selectionHighlightBackground": "#ADD6FF26",
		"list.dropBackground": "#A33AFF",
		"activityBarBadge.background": "#A33AFF",
		"statusBar.background": "#A33AFF",
		"sideBarTitle.foreground": "#D4D4D4",
		"input.placeholderForeground": "#A6A6A6",
		"settings.textInputBackground": "#292929",
		"settings.numberInputBackground": "#292929",
		"menu.background": "#252526",
		"menu.foreground": "#CCCCCC"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.quoted.docstring"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#6F6F6F"
			}
		},
		{
			"name": "Functionarg, OtherVar",
			"scope": [
				"variable.parameter.function-call",
				"variable.other.member"
			],
			"settings": {
				"foreground": "#D4D4D4"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"variable.parameter",
				"meta.block",
				"variable.other",
				"meta.function-call.arguments",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF0000"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#FF0000"
			}
		},
		{
			"name": "Preprocessor",
			"scope": [
				"storage.modifier",
				"entity.name.function.preprocessor",
				"meta.preprocessor.macro"
			],
			"settings": {
				"foreground": "#FF7700"
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage.type",
				"support.type.stdint"
			],
			"settings": {
				"foreground": "#00AAFF"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.operator",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#6F6F6F"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#FF7700"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call.python",
				"meta.function-call.generic.python",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#00FF00"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#0055FF"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#FF00FF"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#FFFF00"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"entity.other.inherited-class",
				"support.type",
				"support.class",
				"support.orther.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#00AAFF"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#00AAFF"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
